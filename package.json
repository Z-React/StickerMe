{
  "name": "StickerMe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "sync:firestack": "rsync -avhW --delete --exclude='node_modules' --exclude='.git' --exclude='*.desktop.js' ../../../../react-native/mine/Firestack/ ./node_modules/react-native-firestack/",
    "sync:oauth": "rsync -avhW --delete --exclude='node_modules' --exclude='.git' --exclude='*.desktop.js' ../../../../react-native/mine/OAuthManager/ ./node_modules/react-native-oauth/",
    "sync:billing": "rsync -avhW --delete --exclude='node_modules' --exclude='.git' --exclude='*.desktop.js' ../../../../react-native/mine/react-native-billing/ ./node_modules/react-native-billing/",
    "sync": "npm run sync:firestack && npm run sync:oauth && npm run sync:receipts",
    "build:mkdir": "mkdir -p data/json",
    "build:products": "npm run build:mkdir && babel-node --presets es2015 data/products.js > data/json/products.json"
  },
  "dependencies": {
    "firebase": "^3.2.0",
    "moment": "^2.14.1",
    "react": "15.2.1",
    "react-native": "0.29.1",
    "react-native-app-intro": "^1.1.3",
    "react-native-camera": "git+https://github.com/lwansbrough/react-native-camera.git",
    "react-native-firestack": "^1.1.1",
    "react-native-fs": "^1.5.1",
    "react-native-oauth": "^1.0.6",
    "react-native-vector-icons": "^2.0.3",
    "react-redux": "^4.4.5",
    "redux-module-builder": "0.0.12-0",
    "redux-persist": "^3.2.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "colors": "^1.1.2",
    "firebase-server": "^0.6.0",
    "remote-redux-devtools": "^0.3.3",
    "yargs": "^4.8.0"
  }
}
